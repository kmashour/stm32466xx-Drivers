#ifndef RCC_priv_H_
#define RCC_priv_H_

/*******************************************
******** Peripheral Option definitions**
********************************************/


/*****RCC_CFGR OPTIONS***/
#define RCC_SW_HSI 00
#define RCC_SW_HSE 01
#define RCC_SW_PLLP 10
#define RCC_SW_PLLR 11

/*some common macros for RCC*/
#define CLOCKREADY 1
#define CLOCKNOTREADY 0
#define AHB1 1
#define AHB2 2
#define AHB3 3
#define APB1 4
#define APB2 5
#define APB3 6





#endif
